        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-recommended-scss": "^4.3.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-prettier": "^1.2.0",
        "stylelint-scss": "^5.0.1",
        "v-tooltip": "^2.1.3",
        "vue": "^3.3.4",
        "vue-custom-scrollbar": "^1.4.0",
        "vue-js-popover": "^1.2.1",
